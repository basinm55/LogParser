<?xml version="1.0" encoding="utf-8"?>
<LogParser>
	<Profile Name ="Default">
		<Filters>
			<Filter>
				<Patterns>
					<Pattern>CUsbDevice::CUsbDevice: %s</Pattern>
				</Patterns>
				<Properties>
					<Property i="0">
						<Name>Device</Name>						
						<Type>string</Type>
						<Action>New</Action>						
						<Target>this</Target>
					</Property>
				</Properties>
			</Filter>
			<Filter>
				<Patterns>
					<Pattern>CPort::BindUsbDevice port %s CUsbDevice %s</Pattern>
			    </Patterns>
				<Properties>
					<Property i="0">
						<Name>Port</Name>
						<Type>string</Type>						
						<Action>Assign</Action>
						<Target>Port</Target>
					</Property>
					<Property i="1">
						<Name>Device</Name>
						<Type>string</Type>						
						<Action>Locate</Action>
						<Target>this</Target>
					</Property>				
				</Properties>
			</Filter>	
			<Filter>
				<Patterns>
					<Pattern>CUsbipRequest::CUsbipRequest: type %s dev %s -> %s</Pattern>
				</Patterns>
				<Properties>
					<Property i="0">
						<Name>Type</Name>
						<Action>Assign</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
					<Property i="1">
						<Name>DeviceParentOf</Name>
						<Action>Locate</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
					<Property i="2">
						<Name>Request</Name>
						<Action>New</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
				</Properties>
			</Filter>
			<Filter>
				<Patterns>
					<Pattern>CUsbipRequest::Prepare:@%s: %s(%d)%s, req %s</Pattern>
				</Patterns>
				<Properties>
					<Property i="0">
						<Name>Address</Name>
						<Action>Assign</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
					<Property i="1">
						<Name>Function</Name>
						<Target>this</Target>
						<Action>Assign</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
					<Property i="2">
						<Name>Unknown1</Name>
						<Action>drop</Action>
						<Type>dec</Type>
						<Target>this</Target>
					</Property>
					<Property i="3">
						<Name>Unknown2</Name>
						<Action>Drop</Action>
						<Type>dec</Type>
						<Target>this</Target>
					</Property>
					<Property i="4">
						<Name>Reqest</Name>
						<Action>Drop</Action>
						<Type>string</Type>
						<Target>this</Target>
					</Property>
				</Properties>
			</Filter>			
		</Filters>			
	</Profile>
	
	
	<Profile Name ="Second" />
</LogParser>
